@model SMS.Models.mst_term_rules

@{
    ViewBag.Title = "DeleteTermRule";
}



<div class="right_col" role="main">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <h2>Delete Term Rules</h2>


                <h3>Are you sure you want to delete this?</h3>
                <div>
                   
                    <hr />
                    <dl class="dl-horizontal">

                        <dd>
                           
                            @Html.HiddenFor(model => model.evaluation_id)
                            @Html.HiddenFor(model => model.session)
                            @Html.HiddenFor(model => model.class_id)
                            @Html.HiddenFor(model => model.term_id)
                        </dd>



                    </dl>

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-actions no-color">
                            <input type="submit" value="Delete" class="btn btn-default please_wait" /> 
                            @Html.ActionLink("Back to List", "AllTermRuleList", new { area = "" }, new { @class = "btn btn-primary please_wait" })
                        </div>
                    }
                </div>
                </div>
</div>
</div>
</div>
